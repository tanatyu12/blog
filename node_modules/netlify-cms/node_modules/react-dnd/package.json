{
	"name": "react-dnd",
	"version": "7.3.2",
	"description": "Drag and Drop for React",
	"main": "lib/cjs/index.js",
	"types": "lib/cjs/index.d.ts",
	"repository": {
		"type": "git",
		"url": "https://github.com/react-dnd/react-dnd.git"
	},
	"license": "MIT",
	"scripts": {
		"clean": "rimraf lib dist",
		"bundle:unmin": "webpack --mode development --output-filename=ReactDnD.js",
		"bundle:min": "webpack --mode production --output-filename=ReactDnD.min.js",
		"transpile:cjs": "tsc -b tsconfig.cjs.json",
		"transpile:esm": "tsc -b tsconfig.esm.json",
		"transpile": "run-p transpile:*",
		"build": "run-p bundle:* transpile",
		"test": "run-s clean build",
		"watch": "tsc -w --preserveWatchOutput",
		"start": "npm run watch"
	},
	"dependencies": {
		"dnd-core": "^7.2.0",
		"hoist-non-react-statics": "^3.3.0",
		"invariant": "^2.1.0",
		"lodash": "^4.17.11",
		"recompose": "^0.30.0",
		"shallowequal": "^1.1.0"
	},
	"devDependencies": {
		"@babel/cli": "^7.2.3",
		"@babel/core": "^7.3.4",
		"@types/react": "^16.8.7",
		"@types/react-dom": "^16.8.2",
		"babel-loader": "^8.0.5",
		"npm-run-all": "^4.1.5",
		"react": "^16.8.4",
		"react-dom": "^16.8.4",
		"rimraf": "^2.6.3",
		"ts-loader": "^5.3.3",
		"typescript": "^3.3.3333",
		"webpack": "^4.29.6",
		"webpack-cli": "^3.2.3"
	},
	"peerDependencies": {
		"react": ">= 16.8",
		"react-dom": ">= 16.8"
	},
	"gitHead": "e3fb069a3f6eac67821cd24c487f5e580b728abf"
}
